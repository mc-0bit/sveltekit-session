{
	"name": "sveltekit-session",
	"description": "A session management library for SvelteKit",
	"version": "0.1.3",
	"keywords": [
		"svelte",
		"sveltekit",
		"session",
		"redis",
		"sveltekit-session"
	],
	"homepage": "https://github.com/mc-0bit/sveltekit-session",
	"author": "mc-0bit",
	"repository": {
		"type": "git",
		"url": "https://github.com/mc-0bit/sveltekit-session.git"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./redis": {
			"types": "./dist/redis.d.ts",
			"svelte": "./dist/redis.js",
			"default": "./dist/redis.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"redis": [
				"./dist/redis.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^2.0.2",
		"@types/lodash": "^4.14.192",
		"@types/uid-safe": "^2.1.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"ioredis": "^5.3.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"type-fest": "^3.7.2",
		"typescript": "^5.0.0",
		"vite": "^4.2.0",
		"vitest": "^0.29.8"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"lodash": "^4.17.21",
		"uid-safe": "^2.1.5"
	}
}
